{
  "package": {
    "name": "plug",
    "repo": "distributions",
    "subject": "plug-obp",
    "desc": "Plug distribution",
    "vcs_url": "https://github.com/plug-obp",
    "website_url": "https://github.com/plug-obp",
    "licenses": [
      "MIT"
    ],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "${version}",
    "released": "${date}",
    "gpgSign": false
  },
  "files": [
    {
      "includePattern": "${project}/build/distributions/plug-${project}-${version}.zip",
      "uploadPattern": "plug-${project}-${version}.zip",
      "matrixParams": {
        "override": 1
      }
    }
  ],
  "publish": true
}
